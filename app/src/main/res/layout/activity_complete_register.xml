<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/windmill3"
    tools:context=".CompleteRegisterActivity">


    <EditText
        android:id="@+id/phonenumber"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:fontFamily="@font/s1"

        android:hint="Phone Number"
        android:inputType="text"
        android:textColor="#ffffff"
        android:textColorHighlight="@android:color/white"
        android:textColorHint="@android:color/white"
        android:textColorLink="@android:color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/jobnumber" />


    <Button
        android:id="@+id/birthbutton"
        android:layout_width="160dp"
        android:layout_height="wrap_content"

        android:layout_marginStart="32dp"
        android:layout_marginTop="92dp"

        android:layout_marginEnd="26dp"
        android:background="#FFEB3B"
        android:fontFamily="@font/s1"
        android:padding="12dp"
        android:text="Select birthdate"
        app:layout_constraintEnd_toStartOf="@+id/birthdate"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nationalnewid" />

    <EditText
        android:id="@+id/birthdate"
        android:layout_width="225dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginTop="96dp"
        android:fontFamily="@font/s1"
        android:hint="Birthdate"
        android:inputType="text"
        android:textColor="#ffffff"
        android:textColorHighlight="@android:color/white"
        android:textColorHint="@android:color/white"
        android:textColorLink="@android:color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/birthbutton"
        app:layout_constraintTop_toBottomOf="@+id/nationalnewid" />

    <Button
        android:id="@+id/upBtnnew"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:background="#FFEB3B"
        android:fontFamily="@font/s1"
        android:padding="12dp"
        android:text="Complete Register"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/birthbutton" />

    <EditText
        android:id="@+id/nationalnewid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/s1"

        android:hint="NationalId"
        android:inputType="text"
        android:textColor="#ffffff"
        android:textColorHighlight="@android:color/white"
        android:textColorHint="@android:color/white"
        android:textColorLink="@android:color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/jobnumber" />

    <EditText
        android:id="@+id/jobnumber"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_marginTop="177dp"
        android:fontFamily="@font/s1"
        android:hint="Job Number"
        android:inputType="text"
        android:textColor="#ffffff"
        android:textColorHighlight="@android:color/white"
        android:textColorHint="@android:color/white"
        android:textColorLink="@android:color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>